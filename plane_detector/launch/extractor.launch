<launch>

	<node pkg="unity_sw_mapping" type="unity_feature" name="unity_feature" output="screen"/>


	<arg name="bag_name" default="6"/>
	<node pkg="rosbag" type="play" name="rosbag" args="/home/sam/lidar_camera/scene/$(arg bag_name).bag"/>


	<node pkg="plane_detector" type="extract_plane" name="extract_plane" output="screen">
	    <param name="data_write_path" type="string" value="$(find plane_detector)/data/scene/$(arg bag_name).json"/>
	</node>

</launch>
